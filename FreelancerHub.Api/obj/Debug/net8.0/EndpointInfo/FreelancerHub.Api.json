{
  "openapi": "3.0.1",
  "info": {
    "title": "FreelancerHub.Api",
    "version": "1.0"
  },
  "paths": {
    "/api/Auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/freelancer/profile": {
      "get": {
        "tags": [
          "FreelancerProfile"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "put": {
        "tags": [
          "FreelancerProfile"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FreelancerProfile"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/FreelancerProfile"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/FreelancerProfile"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/FreelancerSignup/register": {
      "post": {
        "tags": [
          "FreelancerSignup"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "FirstName": {
                    "type": "string"
                  },
                  "LastName": {
                    "type": "string"
                  },
                  "Email": {
                    "type": "string"
                  },
                  "Password": {
                    "type": "string"
                  },
                  "ConfirmPassword": {
                    "type": "string"
                  },
                  "Phone": {
                    "type": "string"
                  },
                  "Country": {
                    "type": "string"
                  },
                  "City": {
                    "type": "string"
                  },
                  "Title": {
                    "type": "string"
                  },
                  "Description": {
                    "type": "string"
                  },
                  "Experience": {
                    "type": "string"
                  },
                  "HourlyRate": {
                    "type": "string"
                  },
                  "Availability": {
                    "type": "string"
                  },
                  "Skills": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "Categories": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "Portfolio": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  },
                  "AgreeToTerms": {
                    "type": "boolean"
                  }
                }
              },
              "encoding": {
                "FirstName": {
                  "style": "form"
                },
                "LastName": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "ConfirmPassword": {
                  "style": "form"
                },
                "Phone": {
                  "style": "form"
                },
                "Country": {
                  "style": "form"
                },
                "City": {
                  "style": "form"
                },
                "Title": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "Experience": {
                  "style": "form"
                },
                "HourlyRate": {
                  "style": "form"
                },
                "Availability": {
                  "style": "form"
                },
                "Skills": {
                  "style": "form"
                },
                "Categories": {
                  "style": "form"
                },
                "Portfolio": {
                  "style": "form"
                },
                "AgreeToTerms": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "FreelancerProfile": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "country": {
            "type": "string",
            "nullable": true
          },
          "city": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "experience": {
            "type": "string",
            "nullable": true
          },
          "hourlyRate": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "availability": {
            "type": "string",
            "nullable": true
          },
          "skills": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "categories": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "portfolioFilePaths": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginDTO": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
}